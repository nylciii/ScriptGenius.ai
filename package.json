{
  "name": "scriptgenius-ai",
  "version": "1.0.0",
  "description": "Video transcription and script generation webapp",
  "main": "server.js",
  "scripts": {
    "setup": "node setup.js",
    "test-setup": "node test-setup.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm run dev",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "start": "node server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["video", "transcription", "ai", "script-generation"],
  "author": "ScriptGenius.ai",
  "license": "MIT"
}
